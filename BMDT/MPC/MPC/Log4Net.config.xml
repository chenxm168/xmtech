      <!--<MaxSizeRollBackups value="10"/>-->
      <!--每个日志文件的最大大小-->
      <!--可用的单位:KB|MB|GB-->
      <!--不要使用小数,否则会一直写入当前日志-->
      <maximumFileSize value="50MB"/>
      <!--日志格式-->

      <rollingStyle value="Date" />
      <!--<datePattern value="yyyyMMdd-HH:mm:ss" /> -->
      <layout type="log4net.Layout.PatternLayout">
        <!--每条日志末尾的文字说明-->
        <!-- <footer value="by 周公" /> -->
        <!--输出格式-->
